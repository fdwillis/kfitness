<div class="page-content header-clear-medium">
	<div data-card-height="230" class="card card-style rounded-m shadow-xl preload-img entered loaded" data-src="images/pictures/18w.jpg" data-ll-status="loaded" style="height: 230px; background-image: url(&quot;images/pictures/18w.jpg&quot;);">
		<div class="card-top mt-4 me-3">
			<a href="/auto-trading?&referredBy=<%= current_user&.present? ? current_user&.uuid : params['referredBy'] %>" class="mt-1 float-end text-uppercase font-900 font-11 btn btn-s rounded-s shadow-l bg-highlight">JOIN NOW</a>
		</div>
		<div class="card-bottom text-center col-12">
			<h1 class="color-white fa-4x mb-4"><%= number_to_currency(@assetsUM, precision: 2) %></h1>
			<p class="color-white opacity-70 font-22 mb-4">Assets Under Management</p>
		</div>

		<!-- <div class="card-bottom">
			<p class="ms-3 font-8 font-500 opacity-50 color-white">You could be trading</p>
		</div>
		<div class="card-bottom">
			<p class="text-end me-3 font-8 font-500 opacity-50 color-white"><i class="fa fa-wifi font-10 rotate-90"></i></p>
		</div> -->
		<div class="card-overlay bg-black opacity-70"></div>
	</div>
</div>

<div class="card card-style">
	<div class="content mb-0">
		<h4 class="font-700 text-uppercase font-12 color-highlight mb-3 mt-n2">Trade Stats</h4>
		<div class="d-flex">
			<div class="w-100 align-self-center ps-3">
				<h6 class="font-14 font-700">Trades<span class="float-end color-highlight-dark"><%= @entriesTrades.size %></span></h6>
				<div class="divider mb-2 mt-1"></div>
				<h6 class="font-14 font-700">Open<span class="float-end color-highlight-dark"><%= @currentTrades %></span></h6>
				<div class="divider mb-2 mt-1"></div>
				<h6 class="font-14 font-700">Close<span class="float-end color-highlight-dark"><%= @partialClose %></span></h6>
			</div>
		</div>
		<div class="divider mt-2 mb-3"></div>


		<div class="row mb-0">
			<div class="col-6">
				<div class="mx-0 mb-3">
					<h6 class="font-12 font-800 text-uppercase opacity-30 text-center">Traders</h6>
					<h3 class="color-highlight-dark font-30 mb-0 text-center"><%= @entriesTrades.map(&:user).uniq.size %></h3>
				</div>
			</div>
			<div class="col-6 text-center">
				<div class="mx-0 mb-3">
					<h6 class="font-12 font-800 text-uppercase opacity-30 text-center">Win Rate</h6>
					<h3 class="color-highlight-dark font-30 mb-0 text-center"><%= number_to_percentage((@partialClose.to_f/@entriesTrades.size.to_f) * 100, precision: 2) %></h3>
				</div>
			</div>
		</div>
		<h6 class="font-12 font-800 text-uppercase opacity-30 text-center mb-4">Traders controls their risk and reward preferences.<br>Oarlin was able to protect <%= number_to_percentage((@partialClose.to_f/@entriesTrades.size.to_f) * 100, precision: 2) %> of the <%= @entriesTrades&.size %> trades entered based on the risk and reward settings traders chose.</h6>










	</div>
</div>
