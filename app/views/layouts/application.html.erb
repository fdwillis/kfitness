<!DOCTYPE html>
<html>
  <head>
    <title>Oarlin</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link rel="stylesheet" type="text/css" href="/assets/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="/assets/style.css">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, viewport-fit=cover" />
    <script type="text/javascript" src="https://platform-api.sharethis.com/js/sharethis.js#property=64443874b4717c0019327d62&product=inline-share-buttons&source=platform" async="async"></script>
  </head>

  <body class="theme-light  page-header-fixed" data-background="none" data-highlight="green1">

    <div id="preloader" class="preloader-hide"><div class="spinner-border color-highlight" role="status"></div></div>

    <div id="page">

      <div class="header header-fixed header-logo-center header-auto-show">
          <a href="index.html" class="header-title">Oarlin</a>
          <a href="#" data-back-button="" class="header-icon header-icon-1"><i class="fa fa-arrow-left"></i></a>
          <a href="#" data-toggle-theme="" class="header-icon header-icon-4"><i class="fa fa-lightbulb"></i></a>
      </div>

      <div id="footer-bar" class="footer-bar-1">
        <% if request.fullpath.include?('/trending') %>
          <a href="/trending" class="active-nav"><i class="fa fa-star"></i><span>Trending</span></a>
        <% else %>
          <a href="/trending"><i class="fa fa-star"></i><span>Trending</span></a>
        <% end %>

        <% if request.fullpath.include?('/tracking') %>
          <a href="/tracking" class="active-nav"><i class="fa fa-paper-plane"></i><span>Tracking</span></a>
        <% else %>
          <a href="/tracking"><i class="fa fa-paper-plane"></i><span>Tracking</span></a>
        <% end %>

        <% if request.fullpath.include?('/discover') %>
          <a href="/discover" class="active-nav"><i class="fa fa-search"></i><span>Discover</span></a>
        <% else %>
          <a href="/discover"><i class="fa fa-search"></i><span>Discover</span></a>
        <% end %>

        <% if request.fullpath.include?('/wishlist') %>
          <a href="/wishlist" class="active-nav"><i class="fa fa-gift"></i><span>Wishlist</span></a>
        <% else %>
          <a href="/wishlist"><i class="fa fa-gift"></i><span>Wishlist</span></a>
        <% end %>

        <% if request.fullpath.include?('/profile') || request.fullpath.include?('/blog') || request.fullpath.include?('/how-it-works') %>
          <a href="#" data-menu="menu-sidebar-right-1" class="active-nav"><i class="fa fa-bars"></i><span>Menu</span></a>
        <% else %>
          <a href="#" data-menu="menu-sidebar-right-1"><i class="fa fa-bars"></i><span>Menu</span></a>
        <% end %>

      </div>



      <div id="menu-sidebar-right-1" class="menu menu-box-right menu-box-detached menu-sidebar" data-menu-width="310" style="display: block; width: 310px;">
        <div class="sidebar-content">

          <div class="bg-theme mx-3 rounded-m shadow-m my-3">
            <div class="d-flex px-2 pb-2 pt-2">
              <% if current_user %>
                <div class="ps-2 align-self-center">
                  <h5 class="ps-1 mb-1 pt-1 line-height-xs font-17"><%= Stripe::Customer.retrieve(current_user.stripeCustomerID)['name'] %></h5>
                  <h6 class="ps-1 mb-0 font-400 opacity-40 font-12">Thank You For Being A Member</h6>
                </div>
              <% else %>
                <div class="ps-2 align-self-center">
                  <h5 class="ps-1 mb-1 pt-1 line-height-xs font-17">Welcome Guest</h5>
                  <h6 class="ps-1 mb-0 font-400 opacity-40 font-12">Become A Member Today</h6>
                </div>
              <% end %>
            </div>
          </div>


          <div class="card card-style">
            <div class="content my-0">
              <h5 class="font-700 text-uppercase opacity-40 font-12 pt-2 mb-0">Account</h5>
              <div class="list-group list-custom-small list-icon-0">
                <a href="/profile">
                  <i class="fa font-12 fa-user gradient-blue rounded-sm color-white"></i>
                  <span>Profile</span>
                  <i class="fa fa-angle-right"></i>
                </a>
              </div>
            </div>
          </div>

          <div class="card card-style">
            <div class="content my-0">
              <h5 class="font-700 text-uppercase opacity-40 font-12 pt-2 mb-0">Company</h5>
              <div class="list-group list-custom-small list-icon-0">
                <a href="#" data-menu="menu-backgrounds">
                  <i class="fa font-12 fa-users gradient-orange rounded-sm color-white"></i>
                  <span>How It Works</span>
                  <i class="fa fa-angle-right"></i>
                </a>
              </div>
              <div class="list-group list-custom-small list-icon-0">
                <a href="/blog" data-menu="menu-backgrounds">
                  <i class="fa font-12 fa-pencil gradient-red rounded-sm color-white"></i>
                  <span>Blog</span>
                  <i class="fa fa-angle-right"></i>
                </a>
              </div>
            </div>
          </div>

        </div>
        <% if current_user %>
          <div class="position-sticky w-100 bottom-0 end-0 pb-1">
            <div class="card card-style mb-3">
              <div class="content my-0 py-">
                <div class="list-group list-custom-small list-icon-0">
                  <a href="<%= destroy_user_session_path %>" class="border-0" data-confirm="Logout?">
                    <i class="fa font-12 fa-sign-out gradient-yellow color-white rounded-sm"></i>
                    <span>Logout</span>
                    <i class="fa fa-angle-right"></i>
                  </a>
                </div>
              </div>
            </div>
          </div>
        <% else %>
          <div class="position-sticky w-100 bottom-0 end-0 pb-1">
            <div class="card card-style mb-3">
              <div class="content my-0 py-">
                <div class="list-group list-custom-small list-icon-0">
                  <a href="<%= new_user_session_path %>" data-toggle-theme="" data-trigger-switch="switch-dark2-mode" class="border-0">
                    <i class="fa font-12 fa-sign-in gradient-green color-white rounded-sm"></i>
                    <span>Login</span>
                    <i class="fa fa-angle-right"></i>
                  </a>
                </div>
              </div>
            </div>
          </div>
        <% end %>
      </div>



















        <div class="content">
          <% if flash[:success] %>
            <div id="toast-2" class="toast text-center toast-tiny col-md-12 toast-top bg-green-dark fade" data-delay="3000" data-autohide="true"><i class="fa fa-info mr-3"></i> <%= flash[:success] %></div>
            <script type="text/javascript">
              setTimeout(function(){
                  $('#toast-2').toast('show');
              }, 100);
            </script>
          <% elsif flash[:notice] %>
            <div id="toast-3" class="toast text-center toast-tiny col-md-12 toast-top bg-blue-dark fade" data-delay="3000" data-autohide="true"><i class="fa fa-info mr-3"></i> <%= flash[:notice] %></div>
            <script type="text/javascript">
              setTimeout(function(){
                  $('#toast-3').toast('show');
              }, 100);
            </script>
          <% elsif flash[:error] %>
            <div id="toast-32" class="toast text-center toast-tiny col-md-12 toast-top bg-red-dark fade" data-delay="3000" data-autohide="true"><i class="fa fa-info mr-3"></i> <%= flash[:error] %></div>
            <script type="text/javascript">
              setTimeout(function(){
                  $('#toast-32').toast('show');
              }, 100);
            </script>
          <% elsif flash[:alert] %>
            <div id="toast-344" class="toast text-center toast-tiny col-md-12 toast-top bg-yellow-dark fade" data-delay="15000" data-autohide="true"><i class="fa fa-info mr-3"></i> <%= flash[:alert] %></div>
            <script type="text/javascript">
              setTimeout(function(){
                  $('#toast-344').toast('show');
              }, 100);
            </script>
          <% end %>
          <div class="page-content">
              <%= yield %>
          </div>
        </div>
        <div class="footer card card-style">
            <a href="#" class="footer-title"><span class="color-highlight">Oarlin</span></a>
            <p class="footer-copyright">Copyright Â© Enabled <span id="copyright-year">2023</span>. All Rights Reserved.</p>
            <p class="footer-links"><a href="#" class="color-highlight">Privacy Policy</a> | <a href="#" class="color-highlight">Terms and Conditions</a> | <a href="#" class="back-to-top color-highlight"> Back to Top</a></p>
            <div class="clear"></div>
        </div>
        <br>
        <br>
        <!-- End of Page Content-->

