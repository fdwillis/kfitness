        <div class="content">
          <% if flash[:success] %>
            <div id="toast-2" class="toast text-center toast-tiny col-md-12 toast-top bg-green-dark fade" data-delay="3000" data-autohide="true"><i class="fa fa-info mr-3"></i> <%= flash[:success] %></div>
            <script type="text/javascript">
              setTimeout(function(){
                  $('#toast-2').toast('show');
              }, 100);
            </script>
          <% elsif flash[:notice] %>
            <div id="toast-3" class="toast text-center toast-tiny col-md-12 toast-top bg-blue-dark fade" data-delay="3000" data-autohide="true"><i class="fa fa-info mr-3"></i> <%= flash[:notice] %></div>
            <script type="text/javascript">
              setTimeout(function(){
                  $('#toast-3').toast('show');
              }, 100);
            </script>
          <% elsif flash[:error] %>
            <div id="toast-32" class="toast text-center toast-tiny col-md-12 toast-top bg-red-dark fade" data-delay="3000" data-autohide="true"><i class="fa fa-info mr-3"></i> <%= flash[:error] %></div>
            <script type="text/javascript">
              setTimeout(function(){
                  $('#toast-32').toast('show');
              }, 100);
            </script>
          <% elsif flash[:alert] %>
            <div id="toast-344" class="toast text-center toast-tiny col-md-12 toast-top bg-yellow-dark fade" data-delay="15000" data-autohide="true"><i class="fa fa-info mr-3"></i> <%= flash[:alert] %></div>
            <script type="text/javascript">
              setTimeout(function(){
                  $('#toast-344').toast('show');
              }, 100);
            </script>
          <% end %>
          <div class="page-content">
            <% if User.find_by(uuid: params['referredBy']).present? %>
              <h1 class="font-700 font-10 opacity-50 text-center mt-5 mb-n5">
                "<%= Stripe::Customer.retrieve(User.find_by(uuid: params['referredBy'])&.stripeCustomerID)['name'].titleize %>" GOT YOU HERE</h1>
            <% end %>
            <% if session['coupon'].present? && Stripe::Coupon.list({limit: 100}).map(&:id).include?(session['coupon']) == true %>
              <div class="align-self-center">
                <h1 class="font-700 font-20 opacity-50 text-center mt-5"><%= Stripe::Coupon.retrieve(session['coupon'])['percent_off'].to_i %>% Off With Code <u><%= session['coupon'] %></u>!<br></h1>
                <p class="mt-2 text-center mb-n5"><%= Stripe::Coupon.retrieve(session['coupon'])['times_redeemed'] %>/<%= Stripe::Coupon.retrieve(session['coupon'])['max_redemptions'] %> Claimed

                <% if current_user %>
                  <a href="<%= update_discount_path %>">Apply Coupon</a>
                <% end %>
                </p>
                <p class="mt-2 text-center mb-n5">
                  Act now, This offer will refresh every 24 hours
                </p>
              </div>
            <% end %>
            <%= yield %>
          </div>
